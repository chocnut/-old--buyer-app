{
  "expo": {
    "name": "Eewoo",
    "slug": "buyer-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "packagerOpts": {
      "sourceExts": [
        "js",
        "ts",
        "jsx",
        "tsx",
        "json"
      ]
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/icon.png",
    "primaryColor": "#F03758",
    "splash": {
      "image": "assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F03758"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}